# Chrome WebDriverの設定
# Chrome実行ファイルのパス
CHROME_BINARY_PATH=/usr/bin/google-chrome
# ChromeDriverのパス
CHROME_DRIVER_PATH=/usr/bin/chromedriver

# APIキー設定
# 外部APIを使用する場合に設定
API_KEY=your_api_key_here

# デバッグモード（True/False）
DEBUG=False

# リトライ設定
# 最大リトライ回数
MAX_RETRIES=3
# リトライ間の待機時間（秒）
RETRY_DELAY=2

# タイムアウト設定（秒）
# ページ読み込みのタイムアウト
PAGE_LOAD_TIMEOUT=30
# 要素を見つけるまでの待機時間
IMPLICIT_WAIT=10